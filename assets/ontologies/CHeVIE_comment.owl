    <owl:AnnotationProperty rdf:about="https://CHeVIE.vn/ontologies/end">
        <rdfs:label xml:lang="en">end</rdfs:label>
        <rdfs:range rdf:resource="https://CHeVIE.vn/ontologies/Statement"/>
        <rdfs:domain rdf:resource="https://CHeVIE.vn/ontologies/Period"/>
    </owl:AnnotationProperty>
    


    <!-- https://CHeVIE.vn/ontologies/ethnicity -->

    <owl:AnnotationProperty rdf:about="https://CHeVIE.vn/ontologies/ethnicity">
        <rdfs:label xml:lang="en">Ethnicity</rdfs:label>
    	<rdfs:label xml:lang="vi">Dân tộc</rdfs:label>
    	<rdfs:comment xml:lang="en">Specifies the ethnic group or background of a Person. This property connects a Person to a Statement containing information about their ethnicity.</rdfs:comment>
    	<rdfs:comment xml:lang="vi">Xác định nhóm dân tộc hoặc nguồn gốc của một Người. Thuộc tính này kết nối một Người với một Phát biểu chứa thông tin về dân tộc của họ.</rdfs:comment>
    	<rdfs:range rdf:resource="https://CHeVIE.vn/ontologies/Statement"/>
    	<rdfs:domain rdf:resource="https://CHeVIE.vn/ontologies/Person"/>
