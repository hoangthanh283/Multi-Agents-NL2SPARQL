    	<rdfs:seeAlso>https://www.w3.org/TR/skos-reference/#narrower</rdfs:seeAlso>
    	<rdfs:range rdf:resource="https://CHeVIE.vn/ontologies/Statement"/>
    	<rdfs:domain rdf:resource="https://CHeVIE.vn/ontologies/Period"/>
    </owl:AnnotationProperty>
    


